QWidget#MaskPanel
{
    min-height: 74dip;
    max-height: 74dip;
    margin: 0;
    padding: 0;
    background-color: rgba(38, 38, 38, 100%);
}

QWidget#MaskListScroll
{
    background-color: transparent;
    border: none;
    margin: 0;
    padding: 0;
}

QWidget#MaskList
{
    background-color: transparent;
    border: none;
    margin: 0;
    padding: 0;
    min-height: 37dip;
    max-height: 37dip;
}

QWidget#MaskPanelRoot
{
    background-color: rgba(38, 38, 38, 100%);
}

QWidget[orintation="vertical"]
{
    min-width: 48dip;
    max-width: 48dip;
    margin: 0;
    padding: 0;
}

QWidget[orintation="horizontal"]
{
    min-height: 48dip;
    max-height: 48dip;
    margin: 0;
    padding: 0;
}


QPushButton[MaskArrow="left"]
{
    min-width:  36dip;
    max-width:  36dip;
    min-height: 36dip;
    max-height: 36dip;

    border-image: url(:/voip/mask_left_100);
}
QPushButton[MaskArrow="left"]:hover
{
    border-image: url(:/voip/mask_left_100_hover);
}
QPushButton[MaskArrow="left"]:hover:pressed
{
    border-image: url(:/voip/mask_left_100_hover);
}
QPushButton[MaskArrow="left"]:disabled
{
    border-image: url(:/voip/mask_left_100_disable);
}

QPushButton[MaskArrow="right"]
{
    min-width:  36dip;
    max-width:  36dip;
    min-height: 36dip;
    max-height: 36dip;

    border-image: url(:/voip/mask_right_100);
}
QPushButton[MaskArrow="right"]:hover
{
    border-image: url(:/voip/mask_right_100_hover);
}
QPushButton[MaskArrow="right"]:hover:pressed
{
    border-image: url(:/voip/mask_right_100_hover);
}
QPushButton[MaskArrow="right"]:disabled
{
    border-image: url(:/voip/mask_right_100_disable);
}


QPushButton[MaskArrow="down"]
{
    border-image: url(:/voip/mask_down_100);
}
QPushButton[MaskArrow="down"]:hover
{
    border-image: url(:/voip/mask_down_100_hover);
}
QPushButton[MaskArrow="down"]:hover:pressed
{
    border-image: url(:/voip/mask_down_100_active);
}
QPushButton[MaskArrow="down"]:disabled
{
    border-image: url(:/voip/mask_down_100_disable);
}

QPushButton[MaskArrow="up"]
{
    border-image: url(:/voip/mask_up_100);
}
QPushButton[MaskArrow="up"]:hover
{
    border-image: url(:/voip/mask_up_100_hover);
}
QPushButton[MaskArrow="up"]:hover:pressed
