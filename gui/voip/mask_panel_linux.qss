    border-image: url(:/voip/mask_up_100_disable);
}


QPushButton[CloseMaskArror="true"]
{
    min-width: 32dip;
    max-width: 32dip;
    min-height: 32dip;
    max-height: 32dip;
    border-image: url(:/voip/close_mask_arrow_100);
}
QPushButton[CloseMaskArror="true"]:hover
{
    border-image: url(:/voip/close_mask_arrow_100_hover);
}
